{
  "name": "flux-angular",
  "version": "4.1.0",
  "description": "A FLUX architecture for Angular JS",
  "main": "dist/flux-angular.cjs.js",
  "main:umd": "dist/flux-angular.umd.js",
  "module": "dist/flux-angular.esm.js",
  "unpkg": "dist/flux-angular.umd.js",
  "browserslist": "defaults",
  "scripts": {
    "bootstrap": "npm ci",
    "build": "rollup -c",
    "develop": "rollup -c -w",
    "format": "run-s format:eslint format:prettier",
    "format:eslint": "eslint --fix \"**/*.js\"",
    "format:prettier": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint \"**/*.js\"",
    "test": "karma start --single-run",
    "karma": "karma start",
    "pretest": "run-s lint build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/christianalfoni/flux-angular.git"
  },
  "author": "Christian Alfoni",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "angular": "^1.6.4",
    "angular-mocks": "^1.6.4",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.0.0",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-rollup-preprocessor": "^7.0.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.17.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "peerDependencies": {
    "angular": "^1.6.4"
  },
  "dependencies": {
    "baobab": "^2.4.3",
    "dispatchr": "^0.3.3"
  }
}
